{
  "name": "uma-cli",
  "version": "1.0.0",
  "description": "CLI untuk mencari data inheritance Umamusume: Pretty Derby",
  "main": "./dist/index.js",
  "bin": {
    "uma-cli": "./dist/index.js"
  },
  "scripts": {
    "build": "tsc -w",
    "start": "node ./dist/index.js",
    "lint": "eslint . --ext .ts",
    "dev": "ts-node-esm src/index.ts",
    "dev:help": "ts-node-esm src/index.ts --help",
    "dev:rank": "ts-node-esm src/index.ts --sort=rank",
    "dev:win": "ts-node-esm src/index.ts --sort=win",
    "dev:sparks": "ts-node-esm src/index.ts --sort=sparks",
    "dev:latest": "ts-node-esm src/index.ts --sort=latest"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/RezkyRizaldi/uma-trainer-finder.git"
  },
  "author": "Muhamad Rezky Rizaldi <rezkyrizaldi30@gmail.com>",
  "license": "MIT",
  "keywords": [
    "uma",
    "umamusume",
    "cli",
    "pretty-derby",
    "inheritance",
    "game"
  ],
  "devDependencies": {
    "@eslint/js": "^9.38.0",
    "@types/inquirer": "^9.0.9",
    "@types/node": "^24.9.1",
    "@types/yargs": "^17.0.33",
    "eslint": "^9.38.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "globals": "^16.4.0",
    "jiti": "^2.6.1",
    "prettier": "^3.6.2",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.46.2"
  },
  "dependencies": {
    "chalk": "^5.6.2",
    "fastest-levenshtein": "^1.0.16",
    "inquirer": "^12.10.0",
    "table": "^6.9.0",
    "yargs": "^18.0.0"
  }
}

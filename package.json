{
  "name": "uma-cli",
  "version": "0.1.0",
  "type": "module",
  "scripts": {
    "build": "tsc",
    "start": "node ./dist/index.js",
    "dev": "ts-node-esm src/index.ts",
    "dev:help": "ts-node-esm src/index.ts --help",
    "dev:rank": "ts-node-esm src/index.ts --sort=rank",
    "dev:win": "ts-node-esm src/index.ts --sort=win",
    "dev:sparks": "ts-node-esm src/index.ts --sort=sparks",
    "dev:latest": "ts-node-esm src/index.ts --sort=latest"
  },
  "devDependencies": {
    "@types/node": "^24.3.1",
    "@types/yargs": "^17.0.33",
    "ts-node": "^10.0.0",
    "typescript": "^5.0.0"
  },
  "dependencies": {
    "chalk": "^5.6.2",
    "fastest-levenshtein": "^1.0.16",
    "inquirer": "^12.9.4",
    "table": "^6.9.0",
    "yargs": "^18.0.0"
  }
}